{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SeoSettings": {
    "Items": [
      {
        "SearchEngine": "Google",
        "BaseUrl": "https://www.google.com.au/search",
        "QueryFormat": "?q={keywords}&start={itemNo}",
        "MaxResults": 100,
        "OuterRepeatingElement": "<a href=\"/url?q="
      },
      {
        "SearchEngine": "Bing",
        "BaseUrl": "https://www.bing.com/search",
        "QueryFormat": "?q={keywords}&first={itemNo}",
        "MaxResults": 100,
        "OuterRepeatingElement": "<a href=\""
      }
    ]
  }
}
